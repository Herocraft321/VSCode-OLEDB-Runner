{
  "name": "oledb-runner",
  "displayName": "OLEDB-Runner",
  "description": "",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension",
  "contributes": {
    "commands": [
      {
        "command": "oledb-runner.configurarRuta",
        "title": "Configurar Ruta - OLEDB"
      },
      {
        "command": "oledb-runner.Ejecutar",
        "title": "Ejecutar Consulta - OLEDB"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "oledb-runner",
          "title": "OLEDB Execution",
          "icon": "./resources/icon.svg"
        }
      ]
    },
    "views": {
      "oledb-runner": [
        {
          "id": "configuration",
          "name": "Configuracion"
        }
      ]
    },"configuration": [
      {
        "title": "configuration",
        "properties": {
          "frontMatter.taxonomy.dateField": {
            "type": "string",
            "default": "date",
            "description": "Specifies the date field name to use in your Front Matter"
          }
        }
      }
    ]
  },
  "scripts": {
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "ts-loader": "^9.5.1",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  }
}
